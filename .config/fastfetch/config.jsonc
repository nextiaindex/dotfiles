{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"source": "$HOME/.config/fastfetch/laptop.txt",
		"type": "file",
		"padding": {
			"top": 1,
			"left": 3,
			"right": 3
		},
		"color": {
			"1": "red"
		}
	},
	"modules": [
		"break",
		{
			"type": "Title",
			"color": {
				"user": "red",
				"host": "red"
			}
		},
		"separator",
		{
			"type": "custom",
			"key": "Laptop",
			"format": "ASUS Vivobook 14 A412DA",
			"keyColor": "red"
		},
		{
			"type": "os",
			"format": "{pretty-name}",
			"keyColor": "red"
		},
		{
			"type": "wm",
			"format": "{pretty-name}",
			"keyColor": "red"
		},
		{
			"type": "shell",
			"format": "{pretty-name}",
			"keyColor": "red"
		},
		{
			"type": "terminal",
			"format": "{pretty-name}",
			"keyColor": "red"
		},
		{
			"type": "terminalfont",
			"format": "{name}",
			"keyColor": "red"
		},
		"break",
		{
			"type": "colors",
			"symbol": "square"
		},
		"break",
		"break",
		"break"
	]
}
