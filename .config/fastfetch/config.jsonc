{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"logo": {
		"source": "$HOME/.config/fastfetch/cowsay-id.txt",
		"type": "file",
		"padding": {
			"top": 1,
			"left": 2
		}	
	},
	"modules": [
		"break",
		"break",
		"title",
		"separator",
		{
			"type": "os",
			"format": "{pretty-name}"
		},
		{
			"type": "wm",
			"format": "{pretty-name}"
		},
		{
			"type": "shell",
			"format": "{pretty-name}"
		},
		{
			"type": "terminal",
			"format": "{pretty-name}"
		},
		{
			"type": "terminalfont",
			"format": "{name}"
		},
		"break",
		"colors",
		"break",
		"break",
		"break"
	]
}
